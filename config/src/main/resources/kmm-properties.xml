<config>

	<!--URLS-->
	<param name="kmm.server.domain">${kmm.server.domain.setting}</param>
	<param name="kmm.server.port">8080</param>
	<param name="kmm.server.context">/kmm</param>
	<param name="rice.server.domain">${rice.server.domain.setting}</param>
	<param name="rice.server.port">8080</param>
	<param name="rice.server.context">${rice.server.context.setting}</param>
	<param name="kfs.server.domain">${kfs.server.domain.setting}</param>
	<param name="kfs.server.port">${kfs.server.port.setting}</param>
	<param name="kfs.server.context">/kfs</param>

	<!--KMM Datasource-->
	<param name="kmm.datasource.url">jdbc:mysql://localhost:3306/kmmdemo</param>
	<param name="kmm.datasource.username">kmmdemo</param>
	<param name="kmm.datasource.password">kmmdemo</param>
	<param name="kmm.datasource.pool.maxActive">50</param>
	<param name="kmm.datasource.pool.minIdle">5</param>
	<param name="kmm.datasource.pool.initialSize">1</param>
	<param name="kmm.datasource.pool.maxSize">2</param>
	<param name="kmm.datasource.pool.minSize">1</param>
	<param name="kmm.datasource.pool.maxWait">30000</param>
	<param name="kmm.datasource.pool.accessToUnderlyingConnectionAllowed">true</param>
	<param name="kmm.datasource.pool.validationQuery">select 1 from dual</param>

	<!--Rice Datasource-->
	<param name="rice.datasource.url">jdbc:mysql://localhost:3306/kuldev</param>
	<param name="rice.datasource.username">kuldev</param>
	<param name="rice.datasource.password">kuldev</param>
	<param name="rice.datasource.connection.timeout">100000</param>
	<param name="rice.datasource.pool.maxSize">2</param>
	<param name="rice.datasource.pool.minSize">1</param>
	<param name="rice.datasource.pool.maxWait">30000</param>


</config>
